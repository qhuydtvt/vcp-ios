# platform :ios, '9.0'

target 'Lok App' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
  pod 'RxSwift',    '~> 4.0'
  pod 'RxCocoa',    '~> 4.0'
  pod 'Alamofire', '~> 4.7'
  pod 'RxGesture'
  pod 'SwiftyJSON', '~> 3.1.4'
  pod 'RxRealm'
  pod 'SDWebImage', '~> 3.8.2'
  pod 'ReachabilitySwift', '~> 3'
  pod 'RxDataSources'
  pod 'RealmSwift'
  pod 'SlideMenuControllerSwift'
  # Pods for Lok App
  
  target 'Lok AppTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'RxBlocking', '~> 4.0'
    pod 'RxTest',     '~> 4.0'
  end

  target 'Lok AppUITests' do
    inherit! :search_paths
    # Pods for testing
    pod 'RxBlocking', '~> 4.0'
    pod 'RxTest',     '~> 4.0'
  end

end
